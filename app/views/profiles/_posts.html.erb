<%#=turbo_frame_tag "profilePosts" do %>
    <div class="O_Posts" data-profile-target="posts">
        <div class="W_Posts">
            <% posts.each do |post| %>
                <%= render partial: 'posts/post-preview', locals: { post: post, show_email: false, } %>
            <% end %>
        </div>
    </div>
<%# end %>